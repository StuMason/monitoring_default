{
	"name": "AlfredNutileInc/b",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
	  "behat/mink": "1.6.*@dev",
	  "behat/mink-goutte-driver": "1.1.*@dev",
	  "behat/mink-selenium2-driver": "1.2.*@dev",
	  "symfony/process": ">=2.3",
	  "symfony/event-dispatcher": ">=2.3",
	  "behat/mink-browserkit-driver": "1.2.*@dev",
	  "behat/mink-extension": "1.3.*@dev",
	  "fabpot/goutte": "~1.0",
	  "guzzlehttp/guzzle": "4.*"
    },
	"require-dev": {
	  "phpunit/phpunit": "~4.0",
	  "mockery/mockery": "0.9.*@dev",
	  "php-vcr/php-vcr": "1.1.5",
	  "php-vcr/phpunit-testlistener-vcr": "1.1.2",
	  "way/laravel-test-helpers": "dev-master",
	  "laracasts/testdummy": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
		    "vendor/iron-io/iron_core/IronCore.class.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist",
	    "bin-dir": "bin/"
	},
	"minimum-stability": "dev"
}
